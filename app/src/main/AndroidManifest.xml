        <activity
            android:name=".MainSelect"
            android:label="@string/title_activity_main_select"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Ps_fragment"
            android:label="@string/title_activity_ps_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Memory_frag"
            android:label="@string/title_activity_memory_frag"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
